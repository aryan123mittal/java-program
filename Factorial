//Factorial of large numbers in java

class Factorial{
    public static void main(String[] args) {
        int n = 10;
        if(n < 0){
            System.out.println("Factorial of this number is not possible");
        }
        else if(n == 0){System.out.println("Factorial of 0 is 1");
        }
        else if(n == 1){
            System.out.println("Factorial of 1 is 1");
        }
        else{
        int factorial = 1;
        for(int i=1 ; i<=n ; i++)
        {
            factorial *= i;
        }
        System.out.printf("Factorial of %d is %d",n,factorial);
        }
    }
}


//time complexity is O(n)
//space complexity is O(1)
